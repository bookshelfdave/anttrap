require 'lib/anttrap.rb'

AntTrap.antproject="RakeFile #{Time.new}"
AntTrap.rake = '/usr/bin/rake'
AntTrap.rakeargs = ["-v"]

desc "The Bar task"
task :Bar do
  puts "Bar!"
end

desc "The Foo Task"
task :Foo do 
     puts "Hello world"
end

